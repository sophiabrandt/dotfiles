{
  "Console log": {
    "prefix": "log",
    "body": [
      "console.log($1)"
    ]
  },
  "Arrow function": {
    "prefix": "af",
    "body": [
      "const ${1:function} = (${2:args}) => {",
        "  $3",
        "}"
    ]
  },
  "Plain function": {
    "prefix": "fn",
    "body": [
      "${1:function} (${2:args}) {",
      "  $3",
      "}"
    ]
  },
  "React class component": {
    "prefix": "reactcc",
    "body": [
      "import React, { Component } from 'react'",
      "import PropTypes from 'prop-types'",
      "",
      "export class ${1:Component} extends Component {",
      "  constructor (props) {",
      "    super(props)",
      "  }",
      "",
      "  render () {",
      "    return (",
      "      $2",
      "    )",
      "  }",
      "}",
      "",
      "//$1.propTypes = {",
      "//}"
    ]
  },
  "React functional component": {
    "prefix": "reactfc",
    "body": [
      "import React from 'react'",
      "import PropTypes from 'prop-types'",
      "",
      "export const ${1:Component} = ({ $2 }) => {",
      "  $3",
      "}",
      "",
      "//$1.propTypes = {",
      "//}"
    ]
  },
  "Describe block": {
    "prefix": "desc",
    "body": [
      "describe('$1', () => {",
      "  $2",
      "})"
    ]
  },
  "Individual test": {
    "prefix": "it",
    "body": [
      "it('$1', () => {",
      "  $2",
      "})"
    ]
  }
}
